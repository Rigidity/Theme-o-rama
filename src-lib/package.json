{
  "name": "theme-o-rama",
  "version": "0.1.0",
  "description": "A TypeScript library for theme management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prettier": "prettier --write ."
  },
  "keywords": [
    "theme",
    "typescript",
    "react",
    "ui",
    "styling"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.6.2",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
